#include<vector>
#include<iostream>
using namespace std;

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
	int t;
	cin>>t;
	while(t--){
		int n, m;
		cin>>n>>m;
		vector<string>grd(n);
		for(int i=0; i<n; ++i){
			string in;
			cin>>in;
			grd[i]=in;
		}
		cout<<"-------\n";
		for(auto &vals: grd)
			cout<<vals<<" "<<endl;
		cout<<"-------\n";
		cout<<"here "<<grd[1][1]<<endl;
	}
}
